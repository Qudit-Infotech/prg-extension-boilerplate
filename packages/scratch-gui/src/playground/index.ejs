<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="google" value="notranslate">
    <link rel="shortcut icon" href="static/FavIcon_Qubits.png">
    <title><%= htmlWebpackPlugin.options.title %></title>
    <% if (htmlWebpackPlugin.options.sentryConfig) { %>
        <!-- Sentry error logging to help with finding bugs -->
        <script src="https://cdn.ravenjs.com/3.22.1/raven.min.js" crossorigin="anonymous"></script>
        <script>
            Raven.config(<%= htmlWebpackPlugin.options.sentryConfig %>).install();
        </script>
        <!-- /Sentry -->
    <% } %>
  </head>
  <style type="text/css">
    .loader{
      /* top: 50%;
      bottom: 50%; */
      /* left: 50%; */
      width: 204px;
      z-index: 2;
      height: 55%;
      display: block;
      margin: 0 auto;
      background : url('static/loader.gif'); 
    }
    .loader_img{
      position: absolute;
      top: 30%;
      /* left: 20%; */
      z-index: 2;
      display: block;
      margin: 0 auto;
      width: 253px;
      height: 204px;
    }
     /* Safari */
     @-webkit-keyframes spin {
       0% { -webkit-transform: rotate(0deg); }
       100% { -webkit-transform: rotate(360deg); }
     }
     @keyframes spin {
       0% { transform: rotate(0deg); }
       100% { transform: rotate(360deg); }
     }
   </style>
  <body onload="loadfunct()" >
    <div id="loader" class="loader"> <img class="loader_img" id="loader_img" src="static/loader.gif" alt=""> </div>
    <script>
        var load = document.getElementById('loader')
        var load_image = document.getElementById('loader_img')
        function loadfunct(){
          load.style.display = 'none';
          load_image.style.display = 'none';
        }
    </script>
  </body>
</html>
